{
  "name": "@thi.ng/api",
  "version": "8.7.2",
  "description": "Common, generic types, interfaces & mixins",
  "type": "module",
  "module": "./index.js",
  "typings": "./index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/thi-ng/umbrella.git"
  },
  "homepage": "https://github.com/thi-ng/umbrella/tree/develop/packages/api#readme",
