    "assert",
    "constants",
    "decorators",
    "generic",
    "interface",
    "mixin",
    "type",
    "typescript"
  ],
  "publishConfig": {
    "access": "public"
  },
  "browser": {
    "process": false,
    "setTimeout": false
  },
  "engines": {
    "node": ">=12.7"
  },
